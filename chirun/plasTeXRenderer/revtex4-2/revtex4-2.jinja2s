name: onlinecite 
type: xml
<span class="cite">
{% for item in obj.citation() %}
	{% if item.idref and item.idref.bibitem.url %}
	    <a href="{{ item.idref.bibitem.url }}" >{{ item }}</a>
	{% else %}
	    {{ item }}
	{% endif %}
{% endfor %}
</span>
